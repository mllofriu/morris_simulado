<launch>
  <env name="PYTHONPATH" value="$(env PYTHONPATH)" /> 

  <arg name="nao_ip" default="$(optenv NAO_IP 127.0.0.1)" />
  <arg name="nao_port" default="$(optenv NAO_PORT 9559)" />

	<node name="nao_camera" type="cameras" pkg="nao_sensors" output="screen">
		<param name="camera_info_url" type="string" value="file://$(find nao_sensors)/share/nao_camera_top_640x480.yaml"/>
	     <param name="frame_rate" type="int" value="5" />
		<param name="resolution" type="int" value="2" />
		<param name="color_space" type="int" value="13" />
		 <param name="camera_switch" type="int" value="0" />
  </node>


    
</launch>
